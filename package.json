{
  "name": "jethro",
  "version": "0.0.0",
  "private": true,
  "description": "Jethro | help you manage church service",
  "scripts": {
    "start": "yarn run start:dev",
    "start:no-mock": "cross-env UMI_UI=none UMI_ENV=dev REACT_APP_ENV=dev MOCK=none umi dev",
    "start:dev": "cross-env UMI_UI=none UMI_ENV=dev REACT_APP_ENV=dev umi dev",
    "start:pre": "cross-env UMI_UI=none UMI_ENV=dev REACT_APP_ENV=pre MOCK=none umi dev",
    "i18n:fetch": "node ./i18n.js",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "yarn run i18n:fetch && umi build",
    "build:dev": "yarn run i18n:fetch && cross-env UMI_ENV=dev umi build",
    "build:test": "yarn run i18n:fetch && cross-env REACT_APP_ENV=test UMI_ENV=test umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "test": "UMI_ENV=test umi test",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.3",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.19",
    "@ant-design/pro-descriptions": "^1.10.25",
    "@ant-design/pro-form": "^1.52.3",
    "@ant-design/pro-layout": "^6.31.7",
    "@ant-design/pro-list": "^1.21.16",
    "@ant-design/pro-table": "^2.61.7",
    "@types/cookie": "^0.4.1",
    "@types/styled-components": "^5.1.19",
    "@umijs/route-utils": "^2.0.4",
    "antd": "4.18.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.27.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "lottie-web": "^5.8.1",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "qs": "^6.10.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-code-editor-editable": "^0.6.3",
    "react-dom": "^17.0.2",
    "react-gapi": "^1.0.1",
    "react-helmet-async": "^1.2.2",
    "styled-components": "^5.3.3",
    "umi": "^3.5.20",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/classnames": "^2.3.0",
    "@types/express": "^4.17.13",
    "@types/gapi": "^0.0.41",
    "@types/gapi.auth2": "^0.0.56",
    "@types/gapi.client": "^1.0.5",
    "@types/gapi.client.sheets": "^4.0.20201029",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@umijs/fabric": "^2.8.1",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.37",
    "@umijs/preset-react": "^2.1.0",
    "@umijs/yorkie": "^2.0.5",
    "airtable": "^0.11.1",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "express": "^4.17.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.4",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.5",
    "postcss-less": "^5.0.0",
    "prettier": "^2.5.1",
    "puppeteer-core": "^13.0.1",
    "react-dev-inspector": "^1.7.1",
    "stylelint": "^13.0.0",
    "typescript": "^4.5.4",
    "webpack-chain": "^6.5.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
